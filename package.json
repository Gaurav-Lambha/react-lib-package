        "@types/node": "^17.0.21",
        "@types/react": "^17.0.37",
        "@types/react-dom": "^17.0.11",
        "antd": "^4.21.4",
        "prettier": "2.7.1",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-router-dom": "^6.3.0",
        "typescript": "^4.5.2"
    },
    "peerDependencies": {
        "@ant-design/icons": "^4.7.0",
        "@auth0/auth0-react": "^1.10.2",
        "antd": "^4.21.4",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-router-dom": "^6.3.0"
    }
